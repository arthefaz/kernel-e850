/ {
	/* game mode - default */
	set3: set@0300{
		frt {	   			/* LIT BIG */
			active-ratio 		= < 50 100>;
		};

		/* Ontime Migration */
		ontime {
			foreground	= <1>;
			top-app		= <1>;

			domain0 {
				cpus = "0-3";
				lower-boundary = <0>;
				upper-boundary = <30>;
			};
			domain1 {
				cpus = "4-7";
				lower-boundary = <5>;
				upper-boundary = <100>;
			};
		};

		ecs { };

		/* cpus_binding applied to core selection */
		cpus_binding {
			target-sched-class = <0x8>;	/* fair */

			background	= "0-3";
		};

		fclamp {
			monitor-group {
				top-app         = <1>;
			};
		};

		cpufreq_gov {
				/* LIT BIG */
			htask-boost = < 50 50 >;
		};
	};
};
